@model Setting

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h1>Genre Edit</h1>
        </div>
        <div class="col-lg-8 mx-auto">
            <form method="post" asp-action="edit">
                <div class="form-group">
                    <label asp-for="Key">Key</label>
                    <input type="text" asp-for="Key" class="form-control" placeholder="Enter Genre Name">
                    <span class="text-danger" asp-validation-for="Key"></span>

                    <label asp-for="Value">Value</label>
                    <input type="text" asp-for="Value" class="form-control" placeholder="Enter Genre Name">
                    <span class="text-danger" asp-validation-for="Value"></span>
                </div>
                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </div>
    </div>
</div>


@section Scripts{
    @await Html.PartialAsync("_ValidationPartialView")
}
